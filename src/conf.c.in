  VALIDATE_PARAM_L(g.rtxdb_truncate_minutes, 1, "rtxdb_truncate_minutes");
  VALIDATE_PARAM_L(g.rtxdb_default_process, 0, "rtxdb_default_process");
  VALIDATE_PARAM_L(g.max_load_ave_hard, 0, "loadavemaxhard");
