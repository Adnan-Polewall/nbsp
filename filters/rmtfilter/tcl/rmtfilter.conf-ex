#
# Two working nodes, "dl385a" and "dl385b"
#
set rmtfilter(nodes) [list "dl385a" "dl385b"];

#
# dl385a
#
# The filters can be specified in the following form
# or indidually as given in the next two lines
#
## set rmtfilter(filters,dl385a) [list gpfilter-manager gisfilter];
#
set rmtfilter(enable,dl385a,gpfilter) 2;
set rmtfilter(enable,dl385a,gisfilter) 1;

# Possible ssh options
#
# set rmtfilter(sshopts,dl385a) "-p 2222";

# What to send to this filter (see more examples below)
# set rmtfilter(fname_uregex,dl385a) ".*";    # all
# set rmtfilter(fname_uregex,dl385a) "_sdus";    # radar

#
# dl385b
#
# set rmtfilter(filters,dl385b) [list dafilter-manager rstfilter];
set rmtfilter(enable,dl385b,dafilter) 2;
set rmtfilter(enable,dl385b,rstfilter) 1;
set rmtfilter(sshopts,dl385a) [list -p 2222];
set rmtfilter(fname_uregex,dl385a) {_sdus};
