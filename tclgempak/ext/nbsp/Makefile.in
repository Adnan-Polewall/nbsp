@include@ @q@../../../Makefile.inc@q@

TCLGEMPAK_LIBS = ../../src/gempak.tcl \
	../../src/gdinfo.tcl \
	../../src/sflist.tcl
TCLGEMPAK_PKGINDEX = ../../src/pkgIndex.tcl
TCLGEMPAK_FILES = ${TCLGEMPAK_LIBS} ${TCLGEMPAK_PKGINDEX}
TCLGEMPAK_INSTALLDIR = ${MYLIBEXECDIR}/tclgempak

INSTALL =       @INSTALL@
TCLSH =		@TCLSH@

all:
	cd ../..; ${MAKE} all

install: all
	${INSTALL} -m 644 ${TCLGEMPAK_FILES} ${TCLGEMPAK_INSTALLDIR}

clean:
	cd ../../; ${MAKE} clean
	rm -f *~ Makefile
